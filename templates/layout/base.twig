{% import "../macro/macro.twig" as macro %}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta name="format-detection" content="telephone=no">
  <meta name="format-detection" content="date=no">
  <meta name="format-detection" content="address=no">
  <meta name="format-detection" content="email=no">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

  <title>{{ title }}</title>

  <style type="text/css">
     {% include "../../public/dist/styles/embed.css" %}
  </style>
  
  <link href="dist/styles/inline.css" rel="stylesheet" type="text/css" />

</head>

<body class="">

  {% include "../partial/header.twig" %}

  <table class="main" class="" border="0" align="center" cellpadding="0" cellspacing="0" width="100%">
    <tr>
      <td class="container" align="center">
        <table class="max-w600" align="center" cellpadding="0" cellspacing="0" border="0" width="600">
          <tr>
            <td>{% block content %}{% endblock %} </td>
          </tr>
        </table>
      </td>
    </tr>
  </table>

  {% include "../partial/footer.twig" %}

</body>

</html>
